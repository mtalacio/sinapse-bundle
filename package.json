  "repository": "https://github.com/nodecg/nodecg.git",
  "license": "Apache-2.0",
  "dependencies": {
    "@emotion/react": "^11.8.1",
    "@emotion/styled": "^11.8.1",
    "@fontsource/roboto": "^4.5.5",
    "@fontsource/rubik": "^4.5.6",
    "@material-ui/icons": "^4.11.3",
    "@mui/material": "^5.4.4",
    "@mui/x-data-grid": "^5.8.0",
    "crypto": "^1.0.1",
    "flux": "^4.0.3",
